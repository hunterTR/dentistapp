<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="users" optimistic-lock="version" polymorphism="implicit" select-before-update="false"/>
   <meta attribute="class-description">
            This class contains the user detail. 
        </meta>
        <id name="id" type="int" column="ID">
            <generator class="native"/>
        </id>
        <property name="rentalType" column="RENTALTYPE" type="string"/>
        <property name="identityNo" column="IDENTITYNO" type="string"/>
        <property name="firstName" column="FIRSTNAME" type="string"/>
        <property name="lastName" column="LASTNAME" type="string"/>
        <property name="phoneNumber" column="PHONENUMER" type="string"/>
        <property name="mail" column="MAIL" type="string"/>
        <property name="city" column="CITY" type="string"/>
        <property name="address" column="ADDRESS" type="string"/>
</hibernate-mapping>
